{
  "name": "@yaoyaoyu/api-project-auto-generator-test",
  "version": "1.0.7",
  "description": "API code generator project template",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "generated/dist/**/*",
    "generated/src/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "generate": "node dist/generate.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "ci:build": "npm run build && npm run generate",
    "ci:publish": "cd generated && npm run build && npm version patch && npm publish --access public",
    "ci:full": "npm run ci:build && npm run ci:publish"
  },
  "keywords": ["api", "generator", "openapi", "swagger"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "fs-extra": "^11.2.0",
    "handlebars": "^4.7.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/fs-extra": "^11.0.4",
    "typescript": "^5.3.3",
    "tsup": "^8.5.0"
  }
}
